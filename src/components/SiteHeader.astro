---
const links = [
  { href: '/', label: 'Home' },
  { href: '/content/', label: 'Content' },
  { href: '/podcasts/', label: 'Podcasts' },
];
---

<header class="mx-auto max-w-prose px-4 py-4">
  <nav aria-label="Primary">
    <ul class="flex gap-4 text-sm text-zinc-600 dark:text-zinc-300">
      {links.map((l) => (
        <li><a href={l.href}>{l.label}</a></li>
      ))}
    </ul>
  </nav>
  
</header>


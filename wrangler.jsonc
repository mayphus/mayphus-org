{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "mayphus-org",
  "compatibility_date": "2025-06-05",
  "assets": {
    "directory": "./dist",
    "not_found_handling": "404-page"
  },
  "routes": [
    {
      "pattern": "mayphus.org",
      "custom_domain": true
    }
  ],
  "r2_buckets": [
    {
      "binding": "MEDIA",
      "bucket_name": "media"
    }
  ]
}
